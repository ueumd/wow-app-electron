{
	"name": "wow-app-electron",
	"version": "1.0.0",
	"description": "An Electron application with Vue and TypeScript",
	"main": "./out/main/index.js",
	"author": "51134642@qq.com",
	"homepage": "https://www.ueumd.com",
	"title": "哇塞",
	"scripts": {
		"mock": "vite --mode mokc --force",
		"format": "prettier --write .",
		"lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
		"typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
		"typecheck:web": "vue-tsc --noEmit -p tsconfig.json --composite false",
		"typecheck": "npm run typecheck:node && npm run typecheck:web",
		"start": "electron-vite preview",
		"dev": "chcp 65001 && electron-vite dev  --watch",
		"build": "electron-vite build",
		"postinstall": "electron-builder install-app-deps",
		"build:win": "npm run build && electron-builder --win --config",
		"build:mac": "npm run build && electron-builder --mac --config",
		"build:linux": "npm run build && electron-builder --linux --config"
	},
	"extraResources": {
		"from": "./server",
		"to": "server"
	},
	"dependencies": {
		"@electron-toolkit/preload": "^2.0.0",
		"@electron-toolkit/utils": "^2.0.0",
		"@ffmpeg-installer/ffmpeg": "^1.1.0",
		"@vueuse/electron": "^10.5.0",
		"axios": "^1.5.1",
		"element-plus": "^2.3.14",
		"fluent-ffmpeg": "^2.1.2",
		"lodash": "^4.17.21",
		"mitt": "^3.0.1",
		"node-media-server": "^2.6.2",
		"nprogress": "^0.2.0",
		"pinia": "^2.1.6",
		"pinia-plugin-persistedstate": "^3.2.0",
		"qs": "^6.11.2",
		"sortablejs": "^1.15.0",
		"vue-i18n": "^9.5.0",
		"vue-router": "^4.2.5"
	},
	"devDependencies": {
		"@electron-toolkit/eslint-config": "^1.0.1",
		"@electron-toolkit/eslint-config-ts": "^1.0.0",
		"@electron-toolkit/tsconfig": "^1.0.1",
		"@rushstack/eslint-patch": "^1.3.3",
		"@types/node": "^18.17.5",
		"@types/nprogress": "^0.2.1",
		"@types/sortablejs": "^1.15.4",
		"@vitejs/plugin-vue": "^4.3.1",
		"@vue/eslint-config-prettier": "^8.0.0",
		"@vue/eslint-config-typescript": "^11.0.3",
		"electron": "25.8.2",
		"electron-builder": "^24.6.3",
		"electron-log": "^4.4.8",
		"electron-updater": "^6.1.4",
		"electron-vite": "^1.0.27",
		"eslint": "^8.47.0",
		"eslint-plugin-vue": "^9.17.0",
		"flv.js": "^1.6.2",
		"fs-extra": "^11.1.1",
		"less": "^4.2.0",
		"mockjs": "^1.1.0",
		"prettier": "^3.0.2",
		"sass": "^1.69.2",
		"typescript": "^5.1.6",
		"unplugin-auto-import": "^0.16.6",
		"vite": "^4.4.9",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-mkcert": "^1.17.1",
		"vite-plugin-mock": "2.9.8",
		"vite-plugin-svg-icons": "^2.0.1",
		"vite-plugin-vue-setup-extend": "^0.4.0",
		"vue": "^3.3.4",
		"vue-tsc": "^1.8.8"
	}
}
